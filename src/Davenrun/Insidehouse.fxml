<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Davenrun.InsidehouseController">
    <children>
       <TextArea fx:id="prompt" layoutX="100" layoutY="0.0" />
        <Button fx:id="Go_outside" layoutX="100" layoutY="250.0" mnemonicParsing="false" text="Go outside" onAction="#Go_Outsidehouse"  />
        <Button fx:id="Sleep" layoutX="100" layoutY="300.0" mnemonicParsing="false" text="Go back to bed" onAction= "#Savegame"/>
        <Button fx:id="exit" layoutX="100" layoutY="350.0" mnemonicParsing="false" text="Exit" onAction = "#Exit"/>
       
    </children>
</AnchorPane>
